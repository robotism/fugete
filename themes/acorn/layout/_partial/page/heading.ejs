<div class="page-heading">
  <script defer src="https://umami.harmonychaos.com/vivoghiRoo2eiRu7" data-website-id="d201db0b-a180-4113-8bf5-dad74e196864"></script>
  <!-- page-type-title -->
  <% if (!is_post()) { %>
    <%- partial('title') %>
  <% } %>
  <!-- page-type-slogan -->
  <h5 class="slogan font-w-lighter text-gray">
    <% if (is_archive()) { %>
    <%- theme.slogan.archives %>
    <% } else if (is_page()) { %>
    <%- theme.slogan.pages %>
    <% } else {%>
    <%- theme.slogan.posts %>
    <% } %>
  </h5>
</div>
